<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".Activity.check_up_from_juinor_choosepatient">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView_dentalhistory"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar2"
        tools:listitem="@layout/item_check_up" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Check Up"
        android:fontFamily="@font/cairo"
        app:titleTextColor="@color/white" />
    <TextView
        android:fontFamily="@font/cairo"
        android:id="@+id/textNoitem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No Item...."
        android:textColor="@color/colorPrimary"
        android:textSize="30sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerView_dentalhistory"
        app:layout_constraintEnd_toEndOf="@+id/recyclerView_dentalhistory"
        app:layout_constraintStart_toStartOf="@+id/recyclerView_dentalhistory"
        app:layout_constraintTop_toTopOf="@+id/recyclerView_dentalhistory" />

</androidx.constraintlayout.widget.ConstraintLayout>