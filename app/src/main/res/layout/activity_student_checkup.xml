<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context=".Activity.StudentCheckup">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="692dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline23"
        app:srcCompat="@drawable/rectangle" />

    <CheckBox
      android:id="@+id/endodontics"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="8dp"
      android:layout_marginEnd="8dp"
      android:layout_marginRight="8dp"
      app:layout_constraintBottom_toTopOf="@+id/guideline21"
      app:layout_constraintEnd_toStartOf="@+id/guideline19"
      app:layout_constraintTop_toBottomOf="@+id/fixedProsthodontics" />

  <CheckBox
      android:id="@+id/prosthesis"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="8dp"
      android:layout_marginRight="8dp"
      app:layout_constraintBottom_toTopOf="@+id/guideline20"
      app:layout_constraintEnd_toStartOf="@+id/guideline19"
      app:layout_constraintTop_toBottomOf="@+id/profile_image"
      app:layout_constraintVertical_bias="0.59000003" />

  <CheckBox
      android:id="@+id/fixedProsthodontics"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="8dp"
      android:layout_marginRight="8dp"
      app:layout_constraintBottom_toTopOf="@+id/guideline3"
      app:layout_constraintEnd_toStartOf="@+id/guideline19"
      app:layout_constraintTop_toBottomOf="@+id/amalgam" />

  <CheckBox
      android:id="@+id/composite"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="8dp"
      android:layout_marginRight="8dp"
      app:layout_constraintBottom_toTopOf="@+id/guideline5"
      app:layout_constraintEnd_toStartOf="@+id/guideline19"
      app:layout_constraintTop_toTopOf="@+id/guideline21"
      app:layout_constraintVertical_bias="0.25" />

  <CheckBox
      android:id="@+id/amalgam"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="8dp"
      android:layout_marginRight="8dp"
      app:layout_constraintBottom_toTopOf="@+id/guideline4"
      app:layout_constraintEnd_toStartOf="@+id/guideline19"
      app:layout_constraintTop_toTopOf="@+id/guideline20"
      app:layout_constraintVertical_bias="0.56" />

  <de.hdodenhof.circleimageview.CircleImageView
      android:id="@+id/profile_image"
      android:layout_width="80dp"
      android:layout_height="80dp"
      android:src="@drawable/photo"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.04" />

  <TextView
      android:fontFamily="@font/cairo"
      android:id="@+id/Textprosthesis"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginLeft="8dp"
      android:text="- Prosthesis "
      android:textSize="12sp"
      app:layout_constraintBottom_toTopOf="@+id/guideline20"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/profile_image"
      app:layout_constraintVertical_bias="0.6" />

  <TextView
      android:fontFamily="@font/cairo"
      android:id="@+id/amalgamText"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginLeft="8dp"
      android:text="- Operative(amalgam)"
      android:textSize="12sp"
      app:layout_constraintBottom_toTopOf="@+id/guideline4"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/guideline20" />

  <TextView
      android:fontFamily="@font/cairo"
      android:id="@+id/fixedProsthodonticsText"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginLeft="8dp"
      android:text="- Fixed prosthodontics"
      android:textSize="12sp"
      app:layout_constraintBottom_toTopOf="@+id/guideline3"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/amalgam"
      app:layout_constraintVertical_bias="0.65999997" />

  <TextView
      android:fontFamily="@font/cairo"
      android:id="@+id/textView6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginLeft="8dp"
      android:text="-Endodontics "
      android:textSize="12sp"
      app:layout_constraintBottom_toTopOf="@+id/guideline21"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/guideline3" />

  <TextView
      android:fontFamily="@font/cairo"
      android:id="@+id/textView7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginLeft="8dp"
      android:text="-Operative(composite) "
      android:textSize="12sp"
      app:layout_constraintBottom_toTopOf="@+id/guideline5"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/guideline21"
      app:layout_constraintVertical_bias="0.29000002" />

  <TextView
      android:fontFamily="@font/cairo"
      android:id="@+id/prosthesisDate"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginLeft="8dp"
      android:text="date"
      android:textSize="10sp"
      app:layout_constraintBottom_toBottomOf="@+id/Textprosthesis"
      app:layout_constraintStart_toStartOf="@+id/guideline19" />

  <TextView
      android:fontFamily="@font/cairo"
      android:id="@+id/amalgamDate"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginLeft="8dp"
      android:text="date"
      android:textSize="10sp"
      app:layout_constraintBottom_toBottomOf="@+id/amalgam"
      app:layout_constraintStart_toStartOf="@+id/guideline19" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      app:layout_constraintGuide_percent="0.38" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      app:layout_constraintGuide_percent="0.55842185" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      app:layout_constraintGuide_percent="0.31" />

  <TextView
      android:fontFamily="@font/cairo"
      android:id="@+id/fixedProsthodonticsDate"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginLeft="8dp"
      android:text="date"
      android:textSize="10sp"
      app:layout_constraintBottom_toBottomOf="@+id/fixedProsthodonticsText"
      app:layout_constraintStart_toStartOf="@+id/guideline19" />

  <TextView
      android:fontFamily="@font/cairo"
      android:id="@+id/endodonticsDate"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginLeft="8dp"
      android:text="date"
      android:textSize="10sp"
      app:layout_constraintBottom_toBottomOf="@+id/textView6"
      app:layout_constraintStart_toStartOf="@+id/guideline19" />

  <TextView
      android:fontFamily="@font/cairo"
      android:id="@+id/compositeDate"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginLeft="8dp"
      android:text="date"
      android:textSize="10sp"
      app:layout_constraintBottom_toBottomOf="@+id/textView7"
      app:layout_constraintStart_toStartOf="@+id/guideline19" />

  <Button
      android:fontFamily="@font/cairo"
      android:id="@+id/btnchecked"
      style="@style/ButtonStyle"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="16dp"
      android:layout_marginLeft="16dp"
      android:layout_marginEnd="8dp"
      android:layout_marginRight="8dp"
      android:text="Checked"
      android:textAllCaps="false"
      android:textColor="#FFFFFF"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/guideline22"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/guideline5" />

  <Button
      android:fontFamily="@font/cairo"
      android:id="@+id/btnFollow"
      style="@style/ButtonStyle"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="8dp"
      android:layout_marginLeft="8dp"
      android:layout_marginEnd="16dp"
      android:layout_marginRight="16dp"
      android:capitalize="none"
      android:paddingLeft="16dp"
      android:paddingRight="16dp"
      android:text="Continue checking"
      android:textAllCaps="false"
      android:textColor="#FFFFFF"
      android:textSize="14sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="@+id/guideline22"
      app:layout_constraintTop_toTopOf="@+id/guideline5" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline19"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="vertical"
      app:layout_constraintGuide_percent="0.57" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline20"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      app:layout_constraintGuide_percent="0.25" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline21"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      app:layout_constraintGuide_percent="0.4320828" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline22"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="vertical"
      app:layout_constraintGuide_percent="0.50121653" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.09" />
</androidx.constraintlayout.widget.ConstraintLayout>